import java.util.Scanner;

public class Ejercicio18 {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int num, numSiguiente, numAnterior, numPantalla;
		numAnterior = 1;
		numSiguiente = 1;
		
		System.out.println("Esribe un n√∫mero para realizar la secuencua de Fibonacci: ");
		num = entrada.nextInt();
		
		if(num==1) {
			System.out.println("1");
			
		}else {
			numPantalla = 1;
			System.out.print(numAnterior+"  "+numSiguiente+"  ");
			for (int i = 0; i < (num-2); i++) {
				numPantalla = numPantalla + numSiguiente;
				System.out.print(" "+ numPantalla +" ");
				numPantalla = numSiguiente;
				numSiguiente ++;
			}
			
		}
		
	}
}


		

